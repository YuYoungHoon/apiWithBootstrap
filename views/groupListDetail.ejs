<!DOCTYPE html>
<html lang="ko">

<head>
  <title>Atec - Group List Detail</title>
  <%include ../views/common/header.ejs%>
  <%include ../views/common/sysJquery.ejs%>
</head>

<body onload="getgroupDetail();">
  <div id="wrapper">
    <% include ../views/common/navMenu.ejs%>
    <div id="page-wrapper">
      <div class="row">
        <div class="col-lg-12">
          <h1 class="page-header"><i class="fa fa-indent fa-fw"></i>Group List Detail</h1>
        </div>
        <!-- /.col-lg-12 -->
      </div>
      <!-- /.row -->
      <div class="row">
        <div class="col-lg-12">
          <div class="panel panel-default">
            <div class="panel-heading">
                <!-- <a class="btn btn-primary btn-sm " data-toggle="modal" data-target="#addDevice" onclick="">추가</a> -->
                <input type="button" class="btn btn-primary btn-sm " data-toggle="modal" data-target="#addDevice" value="추가"></input>
            </div>

            <!-- /.panel-heading -->
            <div class="panel-body">
              <input type="hidden" id="hiddenInput">
              <table width="100%" class="table table-striped table-bordered table-hover" id="dataTables">
                <thead>
                  <tr>
                    <th>No</th>
                    <th>Device ID</th>
                    <th>Name</th>
                    <th>Device Owner</th>
                    <th>Reg Status</th>
                    <th>Action</th>
                  </tr>
                </thead>
                <tbody>
                  <!-- Modal -->
                  <%include ../views/modalPopUp/groupListModal.ejs%>
                  <!-- Modal -->
                </tbody>

              </table>
            </div>
            <!-- /.panel-body -->
          </div>
          <!-- /.panel -->
        </div>
        <!-- /.col-lg-12 -->
      </div>
      <!-- /.col-lg-6 -->
    </div>
    <!-- /.row -->
  </div>
</body>
</html>
